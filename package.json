{
  "name": "bfast-database",
  "version": "2.0.0-rc.11",
  "main": "dist/bfastDatabase.js",
  "license": "MIT",
  "scripts": {
    "build": "gulp build",
    "build:docker": "gulp buildDocker",
    "test:web-services": "yarn run build && mocha --exit specs/rest/all.specs.js",
    "test:controllers": "yarn run build && npx mocha --exit specs/controller/* ",
    "test:web-services:no-build": "mocha specs/rest/all.specs.js",
    "prestart": "yarn run build",
    "prepublishOnly": "npm run build",
    "publish:beta": "yarn publish --tag beta",
    "start": "node .",
    "start:dev": "gulp devStart",
    "push": "gulp publishContainer",
    "push:latest": "gulp publishContainerLatest",
    "push:beta": "gulp publishContainerBeta"
  },
  "dependencies": {
    "bcrypt": "^5.0.0",
    "bfast-faas": "^1.10.5-beta.0",
    "bfastnode": "^4.0.0-beta.7",
    "formidable": "^1.2.2",
    "http-status-codes": "^2.1.2",
    "jsonwebtoken": "^8.5.1",
    "mime": "^2.4.6",
    "minio": "^7.0.16",
    "mongodb": "^3.6.1",
    "sharp": "^0.26.0",
    "uuid": "^8.3.0"
  },
  "devDependencies": {
    "@types/minio": "^7.0.6",
    "@types/mongodb": "^3.5.26",
    "@types/node": "^14.6.0",
    "axios": "^0.20.0",
    "del": "^5.1.0",
    "glob": "^7.1.6",
    "gulp": "^4.0.2",
    "mocha": "^8.1.2",
    "mongodb-memory-server": "^6.6.6",
    "typescript": "^4.0.2"
  }
}
